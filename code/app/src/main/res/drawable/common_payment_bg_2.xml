<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <corners android:radius="50dp"></corners>
            <stroke
                android:width="1dp"
                android:color="#454341"></stroke>
            <size
                android:width="20dp"
                android:height="20dp"></size>
        </shape>

    </item>

    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="#454341"></stroke>
            <size
                android:width="20dp"
                android:height="20dp"></size>
        </shape>

    </item>

</layer-list>